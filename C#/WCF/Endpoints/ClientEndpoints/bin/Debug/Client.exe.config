<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="bhttp" />
            </basicHttpBinding>
            <customBinding>
                <binding name="https">
                    <binaryMessageEncoding />
                    <httpsTransport />
                </binding>
            </customBinding>
            <netTcpBinding>
                <binding name="ntcp" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:9795/WcfService/basichttp"
                binding="basicHttpBinding" bindingConfiguration="bhttp" contract="hRef.IWcfService"
                name="bhttp" />
            <endpoint address="net.tcp://localhost:9796/WcfService/nettcp"
                binding="netTcpBinding" bindingConfiguration="ntcp" contract="hRef.IWcfService"
                name="ntcp">
                <identity>
                    <userPrincipalName value="VITYA-PC\Vitya" />
                </identity>
            </endpoint>
            <endpoint address="https://localhost:9595/WcfService/nethttps"
                binding="customBinding" bindingConfiguration="https" contract="hRef.IWcfService"
                name="https" />
        </client>
    </system.serviceModel>
</configuration>